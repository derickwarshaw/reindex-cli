{
  "extends": "eslint-config-airbnb",
  "env": {
    "mocha": true,
    "node": true
  },
  "rules": {
    "react/jsx-quotes": 0,
    "block-scoped-var": 0,
    "comma-dangle": [2, "always-multiline"],
    "no-else-return": 0,
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "no-use-before-define": 0,
  }
}
